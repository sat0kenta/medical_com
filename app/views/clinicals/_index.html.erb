<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>症例</th>
      <th>詳細</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>

    <% @clinicals.each do |clinical| %>
      <tr>
        <td>
          <%= link_to(clinical.user) do %>
          <% end %>
        </td>
        <td><%= link_to clinicals_path(clinical.id) %></td>
        <td><%= clinical.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>